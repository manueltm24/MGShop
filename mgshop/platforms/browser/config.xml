<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pucmm.programacionweb2017" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>MGSHOP</name>
    <description>
        Aplicacion movil MGSHOP
    </description>
    <author email="info@pucmm.edu.do" href="http://pucmm.edu.do">
        Gustavo Henriquez y Manuel Tolentino
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
